<Application x:Class="GlassLookTests.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GlassLookTests"
             StartupUri="MainWindow.xaml"
             Startup="Application_Startup">
    <Application.Resources>
        <BitmapImage x:Key="BackGroundGIF" UriSource="pack://application:,,,/Assets/Background_GIF.gif"/>
        <BitmapImage x:Key="BackGroundFuchsia" UriSource="pack://application:,,,/Assets/MeshGradient_Fuchsia.jpg"/>
        <BitmapImage x:Key="BackGroundGreen" UriSource="pack://application:,,,/Assets/MeshGradient_Green.jpg"/>
        <BitmapImage x:Key="flag_placeholder" UriSource="pack://application:,,,/Assets/placeholder_flag.png"/>
    </Application.Resources>
</Application>
